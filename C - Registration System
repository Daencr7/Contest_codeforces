int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int t;cin>>t;
    //string s;
    //cin>>s;
    //cout<<s;
    map <string, int> m;

    while (t--)
    {
        string s;cin>>s;
        if (m[s] == 0){
            cout<<"OK";el;
            m[s] += 1;
        }
        else {
            cout<<s<<m[s];el;
            m[s] += 1;
        }     
    }
        
    return 0;
}
